{
    "streams": [
        {
            "stream": {
                "name": "helium_account_activity",
                "json_schema": {
                    "$schema": "http://json-schema.org/draft-07/schema#",
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        },
                        "time": {
                            "type": "integer"
                        },
                        "start_epoch": {
                            "type": "integer"
                        },
                        "rewards": {
                            "type": "array"
                        },
                        "height": {
                            "type": "integer"
                        },
                        "hash": {
                            "type": "string"
                        },
                        "end_epoch": {
                            "type": "integer"
                        }
                    }
                },
                "supported_sync_modes": [
                    "full_refresh"
                ]
            },
            "sync_mode": "incremental",
            "destination_sync_mode": "overwrite"
        }
    ]
}