{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Coingecko Spec",
    "type": "object",
    "required": [
      "id",
      "start_date"
    ],
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "string",
        "description": "Id to extract from"
      },
      "start_date": {
        "type": "string",
        "description": "Which date to start, format should be %d-%m-%Y"
      }
    }
  }
}